{
    "name": "@geometryzen/jsxgraph",
    "version": "0.0.7",
    "description": "EcmaScript and System module format bundles of jsxgraph",
    "exports": {
        ".": {
            "system": "./system/es2015/index.min.js",
            "default": "./esm/es2015/index.min.js"
        }
    },
    "module": "./esm/es2015/index.min.js",
    "type": "module",
    "scripts": {
        "test": "pnpm run build && concurrently -n w: 'pnpm:test:*'",
        "test:browser": "karma start karma.conf.cjs --single-run",
        "debug:test:browser": "karma start karma.conf.cjs",
        "test:unit": "mocha -r chai/register-expect 'node-test/**/*.*'",
        "test:formatting": "prettier --check .",
        "build": "rollup --config rollup.config.mjs",
        "format": "prettier --write .",
        "release": "release-it",
        "prepublishOnly": "pnpm run build && pinst --disable",
        "postinstall": "husky install",
        "postpublish": "pinst --enable"
    },
    "files": [
        "esm",
        "system"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/geometryzen/esm-bundle-jsxgraph.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/geometryzen/esm-bundle-jsxgraph/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/geometryzen/esm-bundle-jsxgraph#readme",
    "devDependencies": {
        "@rollup/plugin-commonjs": "24.0.0",
        "@rollup/plugin-json": "6.0.0",
        "@rollup/plugin-node-resolve": "15.0.1",
        "chai": "4.3.7",
        "concurrently": "7.6.0",
        "husky": "8.0.3",
        "karma": "6.4.1",
        "karma-firefox-launcher": "2.1.2",
        "karma-jasmine": "5.1.0",
        "jsxgraph": "1.5.0-rc2",
        "mocha": "10.2.0",
        "pinst": "3.0.0",
        "prettier": "2.8.2",
        "pretty-quick": "3.1.3",
        "release-it": "15.6.0",
        "release-it-plugin-esm-bundle": "3.0.0",
        "rollup": "3.10.0",
        "rollup-plugin-terser": "7.0.2"
    }
}
